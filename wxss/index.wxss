/*全局注释样式*/
.free-note{
  font-size:30rpx;
  margin-bottom:50rpx;
  color:#707070;
}
/*左箭头*/
.flex-wrap{
  position: relative;
}
.free-icon-right{
  position: absolute;
  right: 15px;
  top: 50%;
  display: inline-block;
  height: 8px;
  width: 8px;
  border-width: 2px 2px 0 0;
  border-color: #707070;
  border-style: solid;
  transform: translateY(-50%) matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
}

@font-face {
  font-family: 'iconfont';  /* project id 439570 */
  src: url('//at.alicdn.com/t/font_439570_nqon8j832ua0pb9.eot');
